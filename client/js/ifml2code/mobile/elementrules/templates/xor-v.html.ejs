<ul class="tabs" style="overflow: hidden">
    <% for (var i = 0; i < landmarks.length; i += 1) { -%>
    <li class="tab col s6"><a class="cyan-text" data-bind="click: landmark.bind($data, '<%= landmarks[i].id %>'), css: {active: active() === '<%= landmarks[i].id %>'}"><%= landmarks[i].name %></a></li>
    <% } -%>
    <div class="indicator cyan" style="z-index:1"></div>
</ul>
<% for (var i = 0; i < children.length; i+=1) { -%>
<!-- ko if: active() === '<%= children[i].id %>' -->
    <!-- <%= children[i].name %> -->
    <c-<%= children[i].id %> params="context: context"></c-<%= children[i].id %>>
<!-- /ko -->
<% } -%>
<% for (var i = 0; i < events.length; i += 1) { -%>
  <% if (events[i].stereotype == 'system') { -%>
    <c-<%= events[i].id %> params="fire: trigger.bind($data,'<%= events[i].id %>')"></c-<%= events[i].id %>>
  <% } else { -%>
    <a class="waves-effect waves-light btn cyan col s12" data-bind="click: trigger.bind($data, '<%= events[i].id %>')"><%= events[i].name %></a>
  <% } -%>
<% } -%>
